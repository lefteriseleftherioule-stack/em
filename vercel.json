{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "crons": [
    { "path": "/api/sync", "schedule": "30 21 * * 2,5" },
    { "path": "/api/sync", "schedule": "30 22 * * 2,5" }
  ]
}
